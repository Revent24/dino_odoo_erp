<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_dino_bank_account_tree" model="ir.ui.view">
    <field name="name">dino.bank.account.tree</field>
    <field name="model">dino.bank.account</field>
    <field name="arch" type="xml">
      <list string="Bank Accounts" create="false" edit="false" delete="false">
        <field name="name" readonly="1"/>
        <field name="account_number" readonly="1"/>
        <field name="currency_id" readonly="1"/>
        <field name="balance_end_date" string="Date" readonly="1"/>
        <field name="balance_start" string="Start Balance" readonly="1"/>
        <field name="turnover_credit" string="Credit Turnover" readonly="1"/>
        <field name="turnover_debit" string="Debit Turnover" readonly="1"/>
        <field name="balance" string="End Balance" readonly="1"/>
        <field name="last_import_date" readonly="1"/>
        <field name="active" readonly="1"/>
      </list>
    </field>
  </record>

</odoo>
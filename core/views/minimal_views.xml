<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="action_dino_minimal" model="ir.actions.act_window">
    <field name="name">Минимальная модель</field>
    <field name="res_model">dino.minimal</field>
    <field name="view_mode">list,form</field>
  </record>

  <record id="view_dino_minimal_list" model="ir.ui.view">
    <field name="name">dino.minimal.list</field>
    <field name="model">dino.minimal</field>
    <field name="arch" type="xml">
      <list>
        <field name="name"/>
      </list>
    </field>
  </record>

  <record id="view_dino_minimal_form" model="ir.ui.view">
    <field name="name">dino.minimal.form</field>
    <field name="model">dino.minimal</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

</odoo>